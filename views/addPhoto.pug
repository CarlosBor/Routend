doctype html
html
  head
    title Add New Photo
  body
    h1 Add New Photo
    form(action=`/photos/${tripId}` method="POST")
      label(for="url") Photo URL:
      input(type="text" name="url" required)
      br
      label(for="idAuthor") Author (ID):
      input(type="number" name="idAuthor" required)
      br
      button(type="submit") Add Photo
